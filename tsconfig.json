{
  "files": [],
  "references": [
    { "path": "./app" },
    { "path": "./tools" },
    { "path": "./options" },
    { "path": "./snap" },
    { "path": "./tests" },
    { "path": "./prisma" },
    { "path": "./package/cli/web" },
    { "path": "./package/node" }
  ],
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": ["node_modules"]
}
