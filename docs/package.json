{
  "package": "@aether-shield/docs",
  "version": "1.0.0",
  "description": "Comprehensive documentation hub for Aether Shield Firewall platform",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "serve": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{md,mdx,json}\"",
    "format:check": "prettier --check \"**/*.{md,mdx,json}\"",
    "validate": "markdownlint \"**/*.md\" \"**/*.mdx\"",
    "validate:links": "markdown-link-check \"**/*.md\" \"**/*.mdx\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "generate": "node scripts/generate-docs.js",
    "generate:api": "node scripts/generate-api-docs.js",
    "generate:components": "node scripts/generate-component-docs.js",
    "deploy": "npm run build && npm run export",
    "export": "next export",
    "preview": "npm run build && npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "stats": "ANALYZE=true next build",
    "optimize": "next-optimized-images",
    "sitemap": "node scripts/generate-sitemap.js",
    "search-index": "node scripts/generate-search-index.js",
    "dead-links": "node scripts/check-dead-links.js",
    "spell-check": "cspell \"**/*.{md,mdx,ts,tsx,js,jsx}\"",
    "toc": "node scripts/generate-toc.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "semantic-release",
    "sync": "node scripts/sync-docs.js",
    "backup": "node scripts/backup-docs.js",
    "restore": "node scripts/restore-docs.js",
    "health": "node scripts/health-check.js",
    "metrics": "node scripts/docs-metrics.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "documentation",
    "docs",
    "aether-vault",
    "password-manager",
    "digital-vault",
    "security",
    "nextjs",
    "react",
    "typescript",
    "markdown",
    "mdx"
  ],
  "author": {
    "name": "Sky Genesis Enterprise",
    "email": "developer@skygenesisenterprise.com",
    "url": "https://skygenesisenterprise.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skygenesisenterprise/aether-vault.git",
    "directory": "docs"
  },
  "bugs": {
    "url": "https://github.com/skygenesisenterprise/aether-vault/issues"
